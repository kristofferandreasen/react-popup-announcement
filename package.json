{
  "name": "react-popup-announcement",
  "version": "1.0.2",
  "description": "A React popup announcement component.",
  "author": "kristofferandreasen",
  "license": "MIT",
  "repository": "kristofferandreasen/react-popup-announcement",
  "private": false,
  "keywords": [
    "React",
    "TypeScript",
    "JavaScript",
    "Announcement",
    "Banner",
    "Pop-Up"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
